{
  "name": "to-ms",
  "version": "1.2.0",
  "description": "Tiny chained ms creation util",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "nyc mocha --reporter spec --colors --require should --ui bdd",
    "build": "umd toMs -c < ./src/to-ms.js > ./dist/to-ms.js",
    "format": "prettier . --write"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:nathsimpson/to-ms.git"
  },
  "keywords": [
    "ms",
    "milliseconds",
    "chaining",
    "setTimeout",
    "setInterval",
    "time",
    "days",
    "hours",
    "minutes",
    "seconds"
  ],
  "author": "Nathan Simpson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nathsimpson/to-ms/issues"
  },
  "homepage": "https://github.com/nathsimpson/to-ms",
  "devDependencies": {
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "3.0.1",
    "should": "^13.2.3",
    "umd": "^3.0.3"
  }
}
